#include <stdio.h>

int main() {
int arr[100][100],i,j,r,c,sum=0,n;
scanf("%d%d",&r,&c);
for(i=0;i<r;i++)
{
    for(j=0;j<c;j++)
    scanf("%d",&arr[i][j]);
    printf("\n");
}
scanf("%d",&n);
for(j=0;j<r;j++)
{
    
    for(i=0;i<c;i++)
    {
       if(n==j)
    sum=sum+arr[j][i];
    }
}
printf("%d",sum);
   

    return 0;
}
